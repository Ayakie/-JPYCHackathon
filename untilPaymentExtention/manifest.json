{
    "name": "JPYC_inject",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["inject.js"],
            "css": ["./views/style.min.css"]
        }
    ],
    "web_accessible_resources": [{
      "resources": ["ether.js", "abi/abi_JPYC.js", "abi/abi_throwmoneyfactory.js", "index.js", "abi/abi_contract.js", "img/oshi-icon.png", "views/style.min.css", "abi/abi_jpycmultisign.js"],
      "matches": ["<all_urls>"]
    }],
    "host_permissions": ["<all_urls>"]
}
